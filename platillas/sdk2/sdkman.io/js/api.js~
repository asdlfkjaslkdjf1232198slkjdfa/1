var b = ["uno", "dos", "tres"]

function nav(a) {
	incio_html = "<li class=''><a href=";//Aqui se tiene que poner el html
	middle = ">";
	fin_html= "</a></li>";
	r = "";
	for (var i = 0; i < a.length; i++) {
				r += inicio_html + a[i] + middle + a[i] + fin_html; 
		}
	return r;
	}

a = document.getElementById("nav2");
a.innerHTML = nav();